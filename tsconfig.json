{
    "extends": "fastify-tsconfig",
    "compilerOptions": {
        "experimentalDecorators": true,
        "outDir": "dist",
        "allowJs": true,
        "noEmit": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "emitDecoratorMetadata": true,
        "strictPropertyInitialization": false,
        "baseUrl": "./src",
    },
    "include": ["src/**/*.ts"],
}
